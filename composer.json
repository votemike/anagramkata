{
    "name": "votemike/anagramkata",
    "description": "Test your PHPfoo",
    "authors": [
        {
            "name": "Michael Gwynne",
            "email": "michael.gwynne@bbc.co.uk"
        }
    ],
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phpbench/phpbench": "^0.13.0"
    },
    "autoload": {
        "psr-4": {
            "Votemike\\Anagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Votemike\\Anagram\\Test\\": "tests"
        }
    },
    "scripts": {
        "bench": "vendor/bin/phpbench run benchmarks",
        "test": "vendor/bin/phpunit"
    },
    "sort-packages": true
}
